jQuery(document).ready(function($){'use strict';var copyCode=new ClipboardJS('#dm-copy-raw-code',{target:function(trigger){return $(trigger).parent('.dm-buttons-right').parent('.dm-buttons').parent('.control-language').find('#dm-code-raw').get(0);}});copyCode.on('success',function(e){var copyText=$(e.trigger).find('.dm-copy-text');var copyConfirmed=$(e.trigger).find('.dm-copy-confirmed');$(copyText).hide();$(copyConfirmed).show();e.clearSelection();setTimeout(function(){$(copyText).show();$(copyConfirmed).hide();},2500);});copyCode.on('error',function(e){var copyText=$(e.trigger).find('.dm-copy-text');var copyError=$(e.trigger).find('.dm-error-message');e.clearSelection();$(copyText).hide();$(copyError).show();setTimeout(function(){$(copyText).show();$(copyError).hide();},2500);});});